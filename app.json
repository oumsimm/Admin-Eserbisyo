{
  "expo": {
    "name": "Capstone2New",
    "slug": "capstone2new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "capstone2new",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "sdkVersion": "54.0.0",
    "runtimeVersion": "54.0.0",
