    "@types/node": "^20.0.0",
    "@types/prop-types": "^15.7.0",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "react-native-worklets": "^0.5.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
