    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.4",
    "@supabase/supabase-js": "^2.57.4",
    "cross-env": "^10.0.0",
    "expo": "54.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-av": "~16.0.7",
    "expo-barcode-scanner": "~13.0.1",
    "expo-blur": "~15.0.7",
